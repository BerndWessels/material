{
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 explorer version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production": [
      "last 1 chrome version",
      "last 1 explorer version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@aws-amplify/api": "^2.1.3",
    "@aws-amplify/auth": "^2.1.3",
    "@aws-amplify/core": "^2.2.2",
    "@aws-amplify/pubsub": "^2.1.3",
    "@material-ui/core": "^4.9.1",
    "@material-ui/icons": "^4.9.1",
    "prop-types": "^15.7.2",
    "ramda": "^0.27.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hot-loader": "^4.12.19",
    "react-intl": "^3.11.0",
    "react-router-dom": "^5.1.2"
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "plugin:jsx-a11y/recommended",
      "prettier",
      "prettier/react",
      "react-app"
    ],
    "plugins": [
      "jsx-a11y",
      "prettier"
    ],
    "rules": {
      "import/order": "off",
      "no-console": "off",
      "react/forbid-prop-types": "off",
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "sort-imports": "off"
    }
  },
  "name": "material",
  "private": true,
  "scripts": {
    "build": "craco build",
    "eject": "craco eject",
    "start": "craco start",
    "test": "craco test",
    "lint": "eslint .",
    "i18n": "node ./scripts/extract-intl-messages.js"
  },
  "version": "0.1.0",
  "devDependencies": {
    "@craco/craco": "^5.6.3",
    "@hot-loader/react-dom": "^16.11.0",
    "@testing-library/jest-dom": "^5.1.0",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^8.1.0",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "extract-react-intl": "^0.8.1",
    "extract-react-intl-messages": "^2.3.5",
    "handlebars": "^4.7.2",
    "prettier": "^1.19.1",
    "react-scripts": "3.3.1"
  }
